{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React",
  "extends": "./base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "module": "ESNext",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react",
    "importsNotUsedAsValues": "error"
  }
}